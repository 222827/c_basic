#include<stdio.h>


   int main(void)
{
    int a;   // int형 변수 a tjsdjs
    int b, c; // int형 변수b와 c를 동시에 선언
    double da;  //double형 변수 da 선언
    char ch;  // char형 변수 ch tjsdjs

    a = 10;  // a 변수에 정수 10 대입
    b = a; // b변수에  a변수의 값을 대입
    c =a+20; //c변수에 a변수의 값 +20 대입
    ch ='A'; // ch변수에 문자 A대입

    
     return 0;
 
 }
